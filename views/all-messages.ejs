<% messages.forEach(message => { %>
    <a href="/single-message?id=<%= message.id %>">
        <div class="message">
            <div>
                <span><%= message.text %></span>
                <span>by <%= message.username %></span>
            </div>
            <div>
                <span>created at: <%= message.date.toLocaleDateString() %></span>
            </div>
        </div>
    </a>
<% }) %>